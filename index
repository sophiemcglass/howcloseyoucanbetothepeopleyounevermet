var els = document.getElementsByClassName('megaGif');

for (var i = 0; i < els.length; i++) {
    var gif = document.getElementById("foto_giappone");
    var classe = 'megaGif';

    els[i].addEventListener("mouseover", function() {
        gif.classList.add(classe);
    }, false)

    els[i].addEventListener("mouseout", function() {
        gif.classList.remove(classe);
    }, false)}
	
	
	
</script>
